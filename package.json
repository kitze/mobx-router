{
  "name": "mobx-router",
  "version": "0.0.7",
  "description": "A simple router for MobX",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "./node_modules/rollup-babel-lib-bundler/bin/index.js index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitze/mobx-router.git"
  },
  "keywords": [
    "kitze",
    "mobx",
    "router",
    "react"
  ],
  "author": "Kitze <kristijan.mkd@gmail.com> (http://kitze.rocks/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kitze/mobx-router/issues"
  },
  "homepage": "https://github.com/kitze/mobx-router#readme",
  "dependencies": {
    "director": "1.2.8",
    "query-string": "^5.0.0",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.44",
    "@types/react": "^16.0.13",
    "@types/react-dom": "^16.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "jest-cli": "^21.2.1",
    "mobx": "3.3.1",
    "mobx-react": "4.3.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rollup-babel-lib-bundler": "3.1.0"
  },
  "rollupBabelLibBundler": {
    "moduleName": "mobxRouter",
    "external": [
      "react"
    ],
    "namedExports": {
      "node_modules/react/index.js": [
        "createElement"
      ]
    },
    "dest": "dist",
    "babel": {
      "presets": [
        "es2015-rollup",
        "react",
        "stage-0"
      ],
      "plugins": [
        "transform-decorators-legacy"
      ]
    }
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
