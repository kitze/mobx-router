{
  "name": "mobx-router",
  "version": "0.1.0",
  "description": "A simple router for MobX",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "src/",
    "test/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "test": "jest --env=node",
    "build": "del-cli dist && tsc",
    "format": "prettier --print-width 80 --tab-width 4 --single-quote --write '{src,tests}/**/*.js'",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitze/mobx-router.git"
  },
  "keywords": [
    "kitze",
    "mobx",
    "router",
    "react"
  ],
  "author": "Kitze <kristijan.mkd@gmail.com> (http://kitze.rocks/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kitze/mobx-router/issues"
  },
  "homepage": "https://github.com/kitze/mobx-router#readme",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "director": "1.2.8",
    "query-string": "^5.0.1"
  },
  "peerDependencies": {
    "mobx": "^3.x",
    "mobx-react": "^4.x",
    "react": "16.x"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^2.28.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "del-cli": "^1.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.10.0",
    "husky": "^0.14.3",
    "jest": "^21.3.0-beta.14",
    "lint-staged": "^6.0.0",
    "mobx": "3.4.1",
    "mobx-react": "4.3.5",
    "prettier": "^1.7.4",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "setupFiles": [
      "./tests/jestsetup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --print-width 80 --tab-width 4 --single-quote --write",
      "git add"
    ]
  }
}
